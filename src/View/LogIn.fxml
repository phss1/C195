<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="144.0" prefWidth="251.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LogInController">
   <children>
      <TextField fx:id="userNameTxtFld" layoutX="87.0" layoutY="34.0" />
      <Label fx:id="userNameLbl" layoutX="11.0" layoutY="34.0" prefHeight="25.0" prefWidth="67.0" text="User Name" />
      <Label fx:id="passwordLbl" layoutX="25.0" layoutY="72.0" prefHeight="25.0" prefWidth="52.0" text="Password" />
      <Button fx:id="logInBtn" layoutX="126.0" layoutY="107.0" mnemonicParsing="false" onAction="#onActionLogInBtn" text="Log In" />
      <Button fx:id="cancelBtn" layoutX="184.0" layoutY="107.0" mnemonicParsing="false" onAction="#onActionCancelBtn" text="Cancel" />
      <PasswordField fx:id="passwordTxtFld" layoutX="87.0" layoutY="72.0" />
      <Button fx:id="newUserBtn" layoutX="50.0" layoutY="107.0" mnemonicParsing="false" onAction="#onActionNewUserBtn" text="New User" />
      <Label layoutX="10.0" layoutY="10.0" text="Log In Screen" textFill="#100861">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
