<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="376.0" prefWidth="285.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddAppointmentController">
   <children>
      <Label layoutX="10.0" layoutY="10.0" text="Add Appointment" textFill="#100861">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <AnchorPane layoutX="14.0" layoutY="30.0" prefHeight="310.0" prefWidth="256.0">
         <children>
            <Label layoutX="33.0" layoutY="14.0" text="Cust_ID" />
            <TextField fx:id="custIdTxtFld" disable="true" editable="false" layoutX="83.0" layoutY="10.0" />
            <Label layoutX="50.0" layoutY="49.0" text="Title" />
            <TextField fx:id="titleTxtFld" layoutX="83.0" layoutY="45.0" />
            <Label layoutX="13.0" layoutY="82.0" text="Description" />
            <TextField fx:id="descriptionTxtFld" layoutX="83.0" layoutY="78.0" />
            <Label layoutX="28.0" layoutY="114.0" text="Location" />
            <Label layoutX="28.0" layoutY="145.0" text="Contact" />
            <TextField fx:id="contactTxtFld" layoutX="83.0" layoutY="141.0" />
            <Label layoutX="47.0" layoutY="180.0" text="Type" />
            <Label layoutX="51.0" layoutY="214.0" text="URL" />
            <TextField fx:id="urlTxtFld" layoutX="83.0" layoutY="210.0" />
            <Label layoutX="22.0" layoutY="280.0" text="Appt. End" />
            <TextField fx:id="apptEndTxtFld" layoutX="83.0" layoutY="276.0" promptText="mm/dd/yyyy 14:01" />
            <Label layoutX="17.0" layoutY="246.0" text="Appt. Start" />
            <TextField fx:id="apptStartTxtFld" layoutX="83.0" layoutY="242.0" promptText="mm/dd/yyyy 14:01" />
            <ComboBox fx:id="typeComboBx" layoutX="83.0" layoutY="176.0" prefWidth="150.0" />
            <ComboBox fx:id="locationComboBox" layoutX="84.0" layoutY="110.0" prefWidth="150.0" />
         </children>
      </AnchorPane>
      <Button fx:id="saveBtn" layoutX="142.0" layoutY="341.0" mnemonicParsing="false" onAction="#onActionSaveBtn" text="Save" />
      <Button fx:id="cancelBtn" layoutX="193.0" layoutY="341.0" mnemonicParsing="false" onAction="#onActionCancelBtn" text="Cancel" />
   </children>
</AnchorPane>
